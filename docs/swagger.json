{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "High-performance Go service for collecting and serving cryptocurrency quotes (MVRK, USDT, and more)",
        "title": "Mavryk External Data API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:3010",
    "basePath": "/",
    "paths": {
        "/quotes": {
            "get": {
                "description": "Retrieve quotes for MVRK token with optional filters. Returns quotes within the specified time range.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "quotes"
                ],
                "summary": "Get quotes for MVRK token (legacy endpoint)",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Start time (RFC3339 format, e.g., 2025-01-01T00:00:00Z). Default: 24 hours ago",
                        "name": "from",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "End time (RFC3339 format, e.g., 2025-01-01T23:59:59Z). Default: now",
                        "name": "to",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Maximum number of quotes to return. Default: no limit",
                        "name": "limit",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of quotes",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/quotes.Quote"
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request parameters",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/quotes/count": {
            "get": {
                "description": "Retrieve the total number of quotes stored for MVRK token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "quotes"
                ],
                "summary": "Get quotes count for MVRK token (legacy endpoint)",
                "responses": {
                    "200": {
                        "description": "Quote count",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "integer",
                                "format": "int64"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/quotes/last": {
            "get": {
                "description": "Retrieve the most recent quote for MVRK token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "quotes"
                ],
                "summary": "Get latest quote for MVRK token (legacy endpoint)",
                "responses": {
                    "200": {
                        "description": "Latest quote",
                        "schema": {
                            "$ref": "#/definitions/quotes.Quote"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "/{token}": {
            "get": {
                "description": "Retrieve quotes for a specific token (mvrk, usdt, etc.) with optional filters",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "tokens"
                ],
                "summary": "Get quotes for a specific token",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Token name (e.g., mvrk, usdt)",
                        "name": "token",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Start time (RFC3339 format, e.g., 2025-01-01T00:00:00Z). Default: 24 hours ago",
                        "name": "from",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "End time (RFC3339 format, e.g., 2025-01-01T23:59:59Z). Default: now",
                        "name": "to",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Maximum number of quotes to return. Default: no limit",
                        "name": "limit",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of quotes",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/quotes.Quote"
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request parameters",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "404": {
                        "description": "Token not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "quotes.Quote": {
            "type": "object",
            "properties": {
                "btc": {
                    "type": "number"
                },
                "cny": {
                    "type": "number"
                },
                "eth": {
                    "type": "number"
                },
                "eur": {
                    "type": "number"
                },
                "gbp": {
                    "type": "number"
                },
                "jpy": {
                    "type": "number"
                },
                "krw": {
                    "type": "number"
                },
                "timestamp": {
                    "type": "string"
                },
                "usd": {
                    "type": "number"
                }
            }
        }
    }
}